package br.com.alura.challenge;

import java.math.BigDecimal;

public class Moeda {

	//COTAÇÃO DO DIA 05/07/2023 10:52AM
	
	private double valor;
	private double real = 4.86;
	private double euro = 5.29;
	private double libraEstrelina = 6.18;
	private double pesoArgentino = 0.019;
	private double pesoChileno = 0.0061;
	
	public void conversaoParaInternacional(double valor) {
		double valorFinal = this.valor * valor;
	}
	
	public void conversaoParaReal(double  valor) {
		double valorFinal = this.valor / valor;
	}
}
